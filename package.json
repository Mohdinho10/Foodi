    "cors": "^2.8.5",
    "daisyui": "^5.0.35",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.10",
    "stripe": "^18.1.1",
    "validator": "^13.15.0"
